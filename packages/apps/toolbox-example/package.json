{
  "name": "@kadena-dev/toolbox-example",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "format": "pnpm run --sequential /^format:.*/",
    "format:lint": "pnpm run lint:src --fix",
    "format:md": "remark README.md -o --use @kadena-dev/markdown",
    "format:src": "prettier . --cache --write",
    "kadena:prelude": "kadena toolbox prelude",
    "kadena:run": "kadena toolbox run",
    "kadena:start": "kadena toolbox start",
    "kadena:test": "kadena toolbox test",
    "kadena:types": "kadena toolbox types",
    "lint": "pnpm run /^lint:.*/",
    "lint:fmt": "prettier . --cache --check",
    "lint:pkg": "lint-package",
    "lint:src": "eslint src --ext .js,.ts",
    "preview": "NODE_OPTIONS='--import tsx' vite preview",
    "test": "NODE_OPTIONS='--import tsx' vitest run",
    "verify": "tsc --noEmit"
  },
  "dependencies": {
    "@kadena/client": "workspace:*",
    "@kadena/client-utils": "workspace:*",
    "@tanstack/react-query": "^4.35.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@kadena-dev/eslint-config": "workspace:*",
    "@kadena-dev/lint-package": "workspace:*",
    "@kadena-dev/markdown": "workspace:*",
    "@kadena-dev/shared-config": "workspace:*",
    "@kadena/kadena-cli": "workspace:*",
    "@kadena/toolbox": "workspace:*",
    "@kadena/unplugin": "workspace:*",
    "@testing-library/react": "~14.0.0",
    "@testing-library/user-event": "~14.5.1",
    "@types/node": "^18.17.14",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "@vitest/coverage-v8": "^1.1.0",
    "happy-dom": "^12.9.1",
    "tsx": "^4.7.1",
    "typescript": "5.2.2",
    "vite": "^5.1.5",
    "vitest": "^1.1.0",
    "vitest-dom": "^0.1.1"
  }
}
